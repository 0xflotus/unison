type List a = Nil | Cons a (List a)
map : ∀ a b . (a -> b) -> List a -> List b
map f as = case as of
  List.Nil -> List.Nil
  List.Cons h t -> List.Cons h (map f t) -- should not typecheck, missing (f h)
-- definitely should not typecheck!
