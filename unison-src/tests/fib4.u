(+) = (Nat.+)
(<) = (Nat.<)
(-) = (Nat.drop)
(*) = (Nat.*)

fib4 n =
  loop a b c d n = case n of
    0 -> 0
    1 -> d
    n -> loop b c d (a + b + c + d) (n - 1)
  loop 0 0 0 1 n

t = Stream.take 10 (Stream.map fib4 (Stream.fromNat 0))
p = 9000
q = 900
r = 90
s = 9
sum = Stream.foldLeft 0 (+) t
> sum * 10000 + p + q + r + s
